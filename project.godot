; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

_global_script_classes=[{
"base": "MeshInstance3D",
"class": &"VoxelMesh",
"language": &"GDScript",
"path": "res://addons/voxel-core/classes/voxel_mesh/voxel_mesh.gd"
}]
_global_script_class_icons={
"VoxelMesh": "res://addons/voxel-core/classes/voxel_mesh/voxel_mesh.svg"
}

[application]

config/name="Voxel-Core"
config/description="Voxel plugin for the Godot game engine!"
config/features=PackedStringArray("4.0", "Mobile")
boot_splash/image="res://images/voxel-core.splash.png"
boot_splash/bg_color=Color(1, 1, 1, 1)
config/icon="res://images/voxel-core.png"

[editor_plugins]

enabled=PackedStringArray("res://addons/voxel-core/plugin.cfg")

[rendering]

renderer/rendering_method="mobile"
