; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

_global_script_classes=[{
"base": "RefCounted",
"class": &"Voxel",
"language": &"GDScript",
"path": "res://addons/voxel-core/classes/voxel/voxel.gd"
}, {
"base": "MeshInstance3D",
"class": &"VoxelMeshInstance3D",
"language": &"GDScript",
"path": "res://addons/voxel-core/classes/voxel_mesh_instance_3d/voxel_mesh_instance_3d.gd"
}, {
"base": "Resource",
"class": &"VoxelSet",
"language": &"GDScript",
"path": "res://addons/voxel-core/classes/voxel_set/voxel_set.gd"
}, {
"base": "SurfaceTool",
"class": &"VoxelSurfaceTool",
"language": &"GDScript",
"path": "res://addons/voxel-core/classes/voxel_surface_tool/voxel_surface_tool.gd"
}]
_global_script_class_icons={
"Voxel": "res://addons/voxel-core/classes/voxel/voxel.svg",
"VoxelMeshInstance3D": "res://addons/voxel-core/classes/voxel_mesh_instance_3d/voxel_mesh_instance_3d.svg",
"VoxelSet": "res://addons/voxel-core/classes/voxel_set/voxel_set.svg",
"VoxelSurfaceTool": "res://addons/voxel-core/classes/voxel_surface_tool/voxel_surface_tool.svg"
}

[application]

config/name="Voxel-Core"
config/description="Voxel plugin for the Godot game engine!"
config/features=PackedStringArray("4.0", "Mobile")
boot_splash/image="res://images/voxel-core.splash.png"
boot_splash/bg_color=Color(1, 1, 1, 1)
config/icon="res://images/voxel-core.png"

[editor_plugins]

enabled=PackedStringArray("res://addons/voxel-core/plugin.cfg")

[rendering]

renderer/rendering_method="mobile"
